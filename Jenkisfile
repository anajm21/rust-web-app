pipeline {

	agent{
			docker{
				image 'ubunto:latest'
			}
		}

	stages {
        stage('Primer stage') {
            steps {
				sh 'whoami'
                
            }
        }
		
		stage ('Tiempo'){
		
			steps{
				sh 'apt-get update && apt-get install curl -y'
				sh 'curl wttr.in'
			}
		}
	
	}
 }
